<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="aag_medical_reimburse.list">
      <field name="name">aag_medical_reimburse list</field>
      <field name="model">aag_medical_reimburse.aag_medical_reimburse</field>
      <field name="arch" type="xml">
        <tree>
          <field name="idno"/>
          <field name="amount"/>
          <field name="status"/>
          <field name="month"/>
          <field name="year"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="aag_medical_reimburse.action_window">
      <field name="name">aag_medical_reimburse window</field>
      <field name="res_model">aag_medical_reimburse.aag_medical_reimburse</field>
      <field name="view_mode">tree,form</field>
    </record>


